<% if user_signed_in? %>
  <div class="search_form">
    <%= form_with url:search_path, method: :get do |f| %>
      <%= f.text_field :word %>
      <%= f.select :model, ["User","Book"] %>
      <%= f.select :search, ["完全一致","部分一致","前方一致","後方一致"] %>
      <%= f.submit "検索", class: "btn btn-primary" %>
    <% end %>
  </div>
<% end %>
  